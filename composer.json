{
    "name": "aeondigital/phpengarde",
    "description": "Micro framework para criar/gerenciar multiplas aplicações/APIs em um mesmo domínio.",
    "type": "library",
    "minimum-stability": "stable",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=7.1.14",
        "psr/http-message": "dev-master",
        "psr/http-server-middleware": "dev-master",
        "aeondigital/phpcoreextension": "dev-master",
        "aeondigital/phptraits": "dev-master",
        "aeondigital/phpcollection": "dev-master",
        "aeondigital/phpstream": "dev-master",
        "aeondigital/phphttpuri": "dev-master",
        "aeondigital/phphttpdata": "dev-master",
        "aeondigital/phphttpmessage": "dev-master",
        "aeondigital/phpengardeconfig": "dev-master"
    },
    "require-dev": {
        "php": ">=7.1.14",
        "psr/http-message": "dev-master",
        "psr/http-server-middleware": "dev-master",
        "aeondigital/phpcoreextension": "dev-master",
        "aeondigital/phptraits": "dev-master",
        "aeondigital/phpcollection": "dev-master",
        "aeondigital/phpstream": "dev-master",
        "aeondigital/phphttpuri": "dev-master",
        "aeondigital/phphttpdata": "dev-master",
        "aeondigital/phphttpmessage": "dev-master",
        "aeondigital/phpengardeconfig": "dev-master"
    },
    "authors": [
        {
            "name": "Rianna Cantarelli",
            "email": "rianna.aeon@gmail.com",
            "homepage": "http://aeondigital.com.br",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "AeonDigital\\EnGarde\\": "/src",
            "AeonDigital\\EnGarde\\Config\\": "/src/Config",
            "AeonDigital\\EnGarde\\Handlers\\": "/src/Handlers",
            "AeonDigital\\EnGarde\\Interfaces\\": "/src/Interfaces"
        }
    },
    "keywords": [
        "engarde",
        "domain",
        "server",
        "application",
        "router",
        "framework"
    ]
}
