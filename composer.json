{
    "name": "aeondigital/phpengarde",
    "description": "Micro framework para criar/gerenciar multiplas aplicações/APIs em um mesmo domínio.",
    "version": "v0.3.0-alpha",
    "time": "2020-10-14",
    "type": "library",
    "authors": [
        {
            "name": "Rianna Cantarelli",
            "email": "rianna.aeon@gmail.com",
            "homepage": "http://aeondigital.com.br",
            "role": "developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.14",
        "psr/http-message": "v1.0.1",
        "psr/http-server-middleware": "v1.0.1",
        "aeondigital/phpcoreextension": ">=v0.3.0-alpha",
        "aeondigital/phptraits": ">=v0.3.0-alpha",
        "aeondigital/phpcollection": ">=v0.3.0-alpha",
        "aeondigital/phpstream": ">=v0.3.0-alpha",
        "aeondigital/phphttpuri": ">=v0.3.0-alpha",
        "aeondigital/phphttpdata": ">=v0.3.0-alpha",
        "aeondigital/phphttpmessage": ">=v0.3.0-alpha",
        "aeondigital/phpengardeconfig": ">=v0.3.0-alpha",
        "dompdf/dompdf": "dev-master"
    },
    "require-dev": {
        "aeondigital/phpdoc-to-rst": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "src/"
        ]
    },
    "keywords": [
        "engarde",
        "domain",
        "server",
        "application",
        "router",
        "framework"
    ]
}
