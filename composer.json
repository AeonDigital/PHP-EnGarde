{
    "name": "aeondigital/phpengarde",
    "description": "Micro framework para criar/gerenciar multiplas aplicações/APIs em um mesmo domínio.",
    "version": "v0.2.0-alpha",
    "time": "2019-03-05",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Rianna Cantarelli",
            "email": "rianna.aeon@gmail.com",
            "homepage": "http://aeondigital.com.br",
            "role": "developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.14",
        "psr/http-message": "v1.0.1",
        "psr/http-server-middleware": "v1.0.1",
        "aeondigital/phpcoreextension": ">=v0.2.0-alpha",
        "aeondigital/phptraits": ">=v0.2.0-alpha",
        "aeondigital/phpcollection": ">=v0.2.0-alpha",
        "aeondigital/phpstream": ">=v0.2.0-alpha",
        "aeondigital/phphttpuri": ">=v0.2.0-alpha",
        "aeondigital/phphttpdata": ">=v0.2.0-alpha",
        "aeondigital/phphttpmessage": ">=v0.2.0-alpha",
        "aeondigital/phpengardeconfig": ">=v0.2.0-alpha",
        "dompdf/dompdf": "dev-master"
    },
    "require-dev": {
        "php": ">=7.1.14",
        "psr/http-message": "v1.0.1",
        "psr/http-server-middleware": "v1.0.1",
        "aeondigital/phpcoreextension": ">=v0.2.0-alpha",
        "aeondigital/phptraits": ">=v0.2.0-alpha",
        "aeondigital/phpcollection": ">=v0.2.0-alpha",
        "aeondigital/phpstream": ">=v0.2.0-alpha",
        "aeondigital/phphttpuri": ">=v0.2.0-alpha",
        "aeondigital/phphttpdata": ">=v0.2.0-alpha",
        "aeondigital/phphttpmessage": ">=v0.2.0-alpha",
        "aeondigital/phpengardeconfig": ">=v0.2.0-alpha",
        "dompdf/dompdf": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AeonDigital\\EnGarde\\": "/src",
            "AeonDigital\\EnGarde\\Config\\": "/src/Config",
            "AeonDigital\\EnGarde\\Handlers\\": "/src/Handlers",
            "AeonDigital\\EnGarde\\Interfaces\\": "/src/Interfaces"
        }
    },
    "keywords": [
        "engarde",
        "domain",
        "server",
        "application",
        "router",
        "framework"
    ]
}
